@model List<string>
@{
    ViewData["Title"] = "Unique Divisions";
}

<h2>Unique Divisions</h2>

@if (Model.Count > 0)
{
    <table class="table table-striped">

        <tr>
            <th>Division</th>

        </tr>
        @foreach (var division in Model)
        {
            <tr>
                <td>@division</td>

            </tr>
        }
    </table>
}
else
{
    <div> No Divisions added! </div>
}

<br />
<a class="btn btn-primary" role="button" asp-controller="Employee" asp-action="Index" asp-route-id="">Back</a>
<br />
